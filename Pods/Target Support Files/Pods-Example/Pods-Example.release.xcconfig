ALWAYS_EMBED_SWIFT_STANDARD_LIBRARIES = YES
FRAMEWORK_SEARCH_PATHS = $(inherited) "$PODS_CONFIGURATION_BUILD_DIR/HPGrowingTextView" "$PODS_CONFIGURATION_BUILD_DIR/NOCProtoKit" "$PODS_CONFIGURATION_BUILD_DIR/NoChat" "$PODS_CONFIGURATION_BUILD_DIR/Starscream" "$PODS_CONFIGURATION_BUILD_DIR/YYText"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "$PODS_CONFIGURATION_BUILD_DIR/HPGrowingTextView/HPGrowingTextView.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/NOCProtoKit/NOCProtoKit.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/NoChat/NoChat.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/Starscream/Starscream.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/YYText/YYText.framework/Headers"
OTHER_LDFLAGS = $(inherited) -framework "HPGrowingTextView" -framework "NOCProtoKit" -framework "NoChat" -framework "Starscream" -framework "YYText"
OTHER_SWIFT_FLAGS = $(inherited) "-D" "COCOAPODS"
PODS_BUILD_DIR = $BUILD_DIR
PODS_CONFIGURATION_BUILD_DIR = $PODS_BUILD_DIR/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_ROOT = ${SRCROOT}/Pods
